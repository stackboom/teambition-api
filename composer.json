{
    "name": "stackboom/teambition-api",
    "description": "Teambition OpenAPI library for laravel.",
    "keywords": ["composer", "package","library", "laravel", "teambition"],
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "xiaoxiaoyu93",
            "email": "hutongyu@vip.qq.com"
        }
    ],
    "type": "library",
    "require": {
		"php": ">=7.4",
        "laravel/framework": ">=6.0",
        "lcobucci/jwt": ">=4.2",
        "guzzlehttp/guzzle": ">6.0"
    },
    "require-dev": {
		"phpunit/phpunit": ">=8.4"
    },
    "autoload": {
        "psr-4": {
            "Stackboom\\Teambition\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stackboom\\Teambition\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stackboom\\Teambition\\Laravel\\TeambitionServiceProvider"
            ]
        }
    }
}
